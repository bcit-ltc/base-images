{
    "branches": [
        "main"
    ],
    "repositoryUrl": "",
    "dryRun": false,
    "ci": true,
    "debug": false,
    "tagFormat": "${version}",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github",
        [
            "@semantic-release/exec",
            {
                "verifyReleaseCmd": "echo ${nextRelease.version} > NEXT_VERSION"
            }
        ]
    ]
}